{
  "name": "nodebb-plugin-anonymous-categories",
  "version": "0.0.1",
  "description": "A NodeBB plugin adds an option to set categories to be \"anonymous\"",
  "main": "library.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/Gardelll/nodebb-plugin-anonymous-categories"
  },
  "keywords": [
    "nodebb",
    "plugin",
    "anonymous",
    "shell"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "author": {
    "name": "Gardel",
    "email": "sunxinao@hotmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Gardelll/nodebb-plugin-anonymous-categories/issues"
  },
  "readmeFilename": "README.md",
  "nbbpm": {
    "compatibility": "^1.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.2.0",
    "@commitlint/config-angular": "13.2.0",
    "eslint": "7.32.0",
    "eslint-config-nodebb": "0.0.2",
    "eslint-plugin-import": "2.24.2",
    "husky": "7.0.2",
    "lint-staged": "11.1.2"
  }
}
